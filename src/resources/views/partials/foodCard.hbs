<div class='foodCard'>
    <div class='image'>
        <img src={{image}} alt={{name}} loading='lazy' />
    </div>
    <div class='info'>
        <div class='nameFood'>
            <h2>{{name}}</h2>
        </div>
        <div class='description'>
            <h4>{{description}}</h4>
        </div>
        <div class='price'>
            <p>$ {{price}}</p>
        </div>
        <div class='circle'>
            <div class='plus'></div>
        </div>
    </div>
</div>